{
    menuDef
    {
        name "ui_display"
        rect 0 0 640 1 1 0
		decoration
		forecolor 1 1 1 1
        onOpen
        {
            if (dvarstring("ui_display") == "animate_money")
            {
                setLocalVarInt animate_money_start (milliseconds());
            }
            if (dvarstring("ui_display") == "wave_summary")
            {
                setLocalVarInt wave_summary_start (milliseconds());
            }
            close self;
        }
    }
}
